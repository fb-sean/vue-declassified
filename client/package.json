{
  "name": "@heatsrc/vue-declassified",
  "version": "2.0.0",
  "description": "Transform Vue Class Components to Vue 3 script setup",
  "packageManager": "pnpm@8.7.5",
  "engines": {
    "node": ">=18.18.0"
  },
  "files": [
    "dist",
    "LICENSE",
    "README.md"
  ],
  "main": "./dist/main.cjs.js",
  "module": "./dist/main.es.js",
  "types": "./dist/main.d.ts",
  "scripts": {
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "coverage": "vitest --coverage",
    "build": "rimraf dist && vite build",
    "lint": "tsc --noEmit && prettier --check ./**/*.{ts,json,vue}"
  },
  "keywords": [
    "vue-declassified",
    "vuedc",
    "vue-class-component",
    "vue-property-decorator",
    "vuex-class",
    "composition api",
    "script setup",
    "vue",
    "vue 3",
    "transform",
    "cli"
  ],
  "publishConfig": {
    "access": "public"
  },
  "author": "Jared McAteer <jared.mcateer@heatsrc.com>",
  "license": "ISC",
  "peerDependencies": {
    "typescript": "^5.2.2",
    "vue": "^3.3.4",
    "prettier": "^3.0.3"
  },
  "devDependencies": {
    "@vitest/coverage-v8": "^0.34.5",
    "@vitest/ui": "^0.34.5",
    "rimraf": "^5.0.5",
    "ts-clone-node": "^3.0.0",
    "vite": "^4.4.9",
    "vite-plugin-dts": "^3.6.0",
    "vitest": "^0.34.4"
  }
}
